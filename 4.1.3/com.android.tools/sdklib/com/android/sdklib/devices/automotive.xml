<d:devices xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns:d="http://schemas.android.com/sdk/devices/5">
    <d:device>
        <d:name>Automotive (1024p landscape)</d:name>
        <d:id>automotive_1024p_landscape</d:id>
        <d:manufacturer>Google</d:manufacturer>
        <d:hardware>
            <d:screen>
                <d:screen-size>normal</d:screen-size>
                <d:diagonal-length>8.4</d:diagonal-length>
                <d:pixel-density>mdpi</d:pixel-density>
                <d:screen-ratio>notlong</d:screen-ratio>
                <d:dimensions>
                    <d:x-dimension>1024</d:x-dimension>
                    <d:y-dimension>768</d:y-dimension>
                </d:dimensions>
                <d:xdpi>152</d:xdpi>
                <d:ydpi>152</d:ydpi>
                <d:touch>
                    <d:multitouch>basic</d:multitouch>
                    <d:mechanism>finger</d:mechanism>
                    <d:screen-type>capacitive</d:screen-type>
                </d:touch>
            </d:screen>
            <d:networking>
                Bluetooth
                Wifi
                NFC
            </d:networking>
            <d:sensors>
                LightSensor
                GPS
            </d:sensors>
            <d:mic>true</d:mic>
            <d:keyboard>nokeys</d:keyboard>
            <d:nav>nonav</d:nav>
            <d:ram unit="KiB">3774492</d:ram>
            <d:buttons>soft</d:buttons>
            <d:internal-storage unit="KiB">10255672</d:internal-storage>
            <d:cpu>Generic CPU</d:cpu>
            <d:gpu>Generic GPU</d:gpu>
            <d:abi>
                armeabi-v7a
                x86
            </d:abi>
            <d:dock/>
            <d:power-type>plugged-in</d:power-type>
            <d:skin>automotive_1024</d:skin>
        </d:hardware>
        <d:software>
            <d:api-level>28-</d:api-level>
            <d:live-wallpaper-support>false</d:live-wallpaper-support>
            <d:bluetooth-profiles/>
            <d:gl-version>2.0</d:gl-version>
            <d:gl-extensions/>
            <d:status-bar>true</d:status-bar>
        </d:software>
        <d:state name="Landscape" default="true">
            <d:description>The device in landscape orientation</d:description>
            <d:screen-orientation>land</d:screen-orientation>
            <d:keyboard-state>keyssoft</d:keyboard-state>
            <d:nav-state>nonav</d:nav-state>
        </d:state>
        <d:tag-id>android-automotive</d:tag-id>
    </d:device>

    <d:device>
        <d:name>Polestar 2</d:name>
        <d:id>polestar_2</d:id>
        <d:manufacturer>Volvo</d:manufacturer>
        <d:hardware>
            <d:screen>
                <d:screen-size>normal</d:screen-size>
                <d:diagonal-length>11.13</d:diagonal-length>
                <d:pixel-density>mdpi</d:pixel-density>
                <d:screen-ratio>notlong</d:screen-ratio>
                <d:dimensions>
                    <d:x-dimension>1152</d:x-dimension>
                    <d:y-dimension>1536</d:y-dimension>
                </d:dimensions>
                <d:xdpi>172.51</d:xdpi>
                <d:ydpi>172.51</d:ydpi>
                <d:touch>
                    <d:multitouch>basic</d:multitouch>
                    <d:mechanism>finger</d:mechanism>
                    <d:screen-type>capacitive</d:screen-type>
                </d:touch>
            </d:screen>
            <d:networking>
                Bluetooth
                Wifi
                NFC
            </d:networking>
            <d:sensors>
                LightSensor
                GPS
            </d:sensors>
            <d:mic>true</d:mic>
            <d:keyboard>nokeys</d:keyboard>
            <d:nav>nonav</d:nav>
            <d:ram unit="GiB">2</d:ram>
            <d:buttons>soft</d:buttons>
            <d:internal-storage unit="GiB">7811891</d:internal-storage>
            <d:cpu>Generic CPU</d:cpu>
            <d:gpu>Generic GPU</d:gpu>
            <d:abi>
                x86
            </d:abi>
            <d:dock/>
            <d:power-type>plugged-in</d:power-type>
            <d:skin>polestar_2</d:skin>
        </d:hardware>
        <d:software>
            <d:api-level>28-</d:api-level>
            <d:live-wallpaper-support>false</d:live-wallpaper-support>
            <d:bluetooth-profiles/>
            <d:gl-version>2.0</d:gl-version>
            <d:gl-extensions/>
            <d:status-bar>true</d:status-bar>
        </d:software>
        <d:state name="Portrait" default="true">
            <d:description>The device in portrait view</d:description>
            <d:screen-orientation>port</d:screen-orientation>
            <d:keyboard-state>keyssoft</d:keyboard-state>
            <d:nav-state>nonav</d:nav-state>
        </d:state>
        <d:tag-id>android-automotive</d:tag-id>
    </d:device>
</d:devices>
